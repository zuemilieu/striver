class Solution {
public:
void LNR(TreeNode* root, vector<int>& v) {
 if(root == NULL){
   return;
}
LNR(root->left, v);
v.push_back(root->val);
LNR(root->right, v);
}

bool isValidBST(TreeNode* root) {
 vector<int> v;
LNR(root, v);
for(int i =1; i<v.size(); ++i){
 if(v[i]<=v[i-1]){
  return false;
}

}
return true;
}
};
